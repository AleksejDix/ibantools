{
  "name": "ibantools",
  "version": "4.5.1",
  "description": "Validation, extraction and creation of IBAN, BBAN, BIC/SWIFT numbers plus some other helpful stuff like ISO 3136-1 alpha 2 country list",
  "keywords": [
    "IBAN",
    "BBAN",
    "BIC",
    "SEPA",
    "SWIFT",
    "ISO 3136-1 alpha-2"
  ],
  "sideEffects": false,
  "homepage": "https://github.com/Simplify/ibantools",
  "bugs": "https://github.com/Simplify/ibantools/issues",
  "type": "module",
  "main": "./dist/ibantools.js",
  "module": "./dist/ibantools.js",
  "types": "./dist/ibantools.d.ts",
  "exports": {
    ".": {
      "types": "./dist/ibantools.d.ts",
      "import": "./dist/ibantools.js"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Simplify/ibantools.git"
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "prepare": "npm run build",
    "docs": "typedoc src/ibantools.ts",
    "all": "npm run test && npm run lint && npm run docs"
  },
  "author": {
    "name": "Saša Jovanić",
    "url": "https://www.simplify.ba/"
  },
  "license": "MIT or MPL-2.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.6",
    "@typescript-eslint/parser": "^7.6",
    "@vitest/coverage-v8": "^4.0.15",
    "docdash": "^2.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "typedoc": "^0.27.2",
    "typescript": "^5.2",
    "vite": "7.2.7",
    "vite-plugin-dts": "4.5.4",
    "vitest": "^4.0.15"
  }
}
